{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "strict": false,
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": [
      "esnext",
      "dom",
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
